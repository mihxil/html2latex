echo Kreante iksan version...
espilo iso px < fromagxo.tks > fromtks.fun 
echo latex2html
latex2html -split 4 -no_subdir -address '<a href="mailto:mihxil@esperanto.nu">Michiel Meeuwissen &lt;mihxil@esperanto.nu&gt;</a>' fromagxo.tex
rm fromtks.fun
rm index.html
latehx
dvips -f fromtex.dvi > fromagxo.ps
rm fromagxo.ps.gz
echo gzip...
gzip fromagxo.ps
echo Forigante dosierojn...
rm fromtex.dvi
rm *.log
rm *.aux
echo Tradukante anglajxojn...
./traduku.cxiun
